{
    "name": "bugloos/export-bundle",
    "description": "Export everything you need in an easy way",
    "type": "symfony-bundle",
    "license": "proprietary",
    "authors": [
        {
            "name": "Mojtaba Gheytasi",
            "email": "mjgheytasi@gmail.com | mojtaba.g@bugloos.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Bugloos\\ExportBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bugloos\\ExportBundle\\Tests\\": "tests"
        }
    },
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "symfony/framework-bundle": "^4.4 | ^5.4",
        "phpoffice/phpspreadsheet": "^1.17"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "friendsofphp/php-cs-fixer": "^2.0",
        "phpmd/phpmd": "^2.10",
        "squizlabs/php_codesniffer": "^3.6"
    }
}
